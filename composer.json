{
  "name": "spryker/availability-cart-connector",
  "type": "library",
  "description": "AvailabilityCartConnector module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/availability": "^9.0.0",
    "spryker/cart-extension": "^2.1.0 || ^3.0.0 || ^4.0.0",
    "spryker/decimal-object": "^1.0.0",
    "spryker/kernel": "^3.30.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/cart": "If you want to use Cart plugins."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
